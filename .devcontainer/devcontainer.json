{
  "name": "Gitpod_star",
  "build": {
    "context": ".",
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "zsh"
  },
  "extensions": [
    "esbenp.prettier-vscode",
    "dbaeumer.vscode-eslint",
    "bradlc.vscode-tailwindcss",
    "antfu.vite",
    "antfu.vite-inspect"
  ],
  "forwardPorts": [3000],
  "postCreateCommand": "pnpm install"
}
